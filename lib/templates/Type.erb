/* generated by rpcoder */

namespace <%= name_space %>
{
	public class <%= type.name %>
	{
		<%- type.fields.each do |field| -%>
		public <%= field.array_or_type %> <%= field.name.to_s.camelize %> { get; set; }
		<%- end -%>

		public <%= type.name %>() {}

		public <%= type.name %>(<%= type.fields.map {|i| "#{i.array_or_type} #{i.name} = null" }.join(', ') %>)
		{
			<%- type.fields.each do |field| -%>
			this.<%= field.name.to_s.camelize %> = <%= field.name %>;
			<%- end -%>
		}

		public override string ToString()
		{
			string[] resultString = {<%= type.fields.map {|i| "\"#{i.name.to_s.camelize}:\" + #{i.name.to_s.camelize}" }.join(', ') %>};
			return string.Join(", ", resultString);
		}
	}
}
