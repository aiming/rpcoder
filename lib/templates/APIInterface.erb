/* generated by rpcoder */

namespace <%= name_space %>
{
	public interface <%= api_class_name %>Interface
	{
		delegate void handler();
		public delegate void errorHandler(string errorType, string message, string targetScene);
		string BaseUrl { get; set; }
		handler ErrorHandler{ get; set; }
		
		<%- functions.each do |func| -%>
		<%-
			successHandler = func.name.camelize + "Responder"
		-%>
		/**
		* <%= func.description %>
		*
		<%- func.params.each do |param| -%>
		* @<%= param.name %>:<%= param.array_or_type %> <%= param.options[:expect] %> <%= param.options[:description] %>
		<%- end -%>
		* @success:<%= successHandler %>
		* @error:errorHandler
		*/
		public delegate void <%= successHandler %>(<%= func.return_types.map{|i| [i.array_or_type, i.name].join(' ') }.join(', ') %>);
		
		<%-
			params = func.params.map{|i| [i.array_or_type, i.name].join(' ') } + [successHandler + ' success', 'errorHandler error']
		-%>
		public IEnumerator <%= func.name.camelize %>(<%= params.join(', ') %>)
		<%- end -%>
	}
}
